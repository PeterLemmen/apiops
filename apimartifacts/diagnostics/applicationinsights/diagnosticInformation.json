{
  "properties": {
    "alwaysLog": "allErrors",
    "backend": {
      "request": {
        "body": {
          "bytes": 8192
        },
        "headers": [
          "X-Forwarded-For",
          "Authorization"
        ]
      },
      "response": {
        "body": {
          "bytes": 8192
        },
        "headers": [
          "X-Forwarded-For",
          "Authorization"
        ]
      }
    },
    "frontend": {
      "request": {
        "body": {
          "bytes": 8192
        },
        "headers": [
          "X-Forwarded-For",
          "Authorization"
        ]
      },
      "response": {
        "body": {
          "bytes": 8192
        },
        "headers": [
          "X-Forwarded-For",
          "Authorization"
        ]
      }
    },
    "httpCorrelationProtocol": "Legacy",
    "logClientIp": true,
    "sampling": {
      "percentage": 100,
      "samplingType": "Fixed"
    },
    "verbosity": "information"
  }
}